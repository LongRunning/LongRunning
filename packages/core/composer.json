{
    "name": "long-running/core",
    "type": "library",
    "description": "Tools for working with long-running processes",
    "homepage": "http://github.com/LongRunning/LongRunning",
    "license": "MIT",
    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "https://matthiasnoback.nl/"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "conflict": {
        "long-running/long-running": "*"
    },
    "autoload": {
        "psr-4": {
            "LongRunning\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LongRunning\\Core\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
