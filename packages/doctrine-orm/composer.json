    "require": {
        "php": "^7.4 || ^8.0",
        "long-running/core": "dev-main",
        "psr/log": "^1.0.1",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.12",
        "symfony/framework-bundle": "^4.4 || ^5.1",
        "doctrine/doctrine-bundle": "^2.0.10"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<2.0.10"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "When using the Symfony bundle"
    },
    "autoload": {
        "psr-4": {
            "LongRunning\\DoctrineORM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LongRunning\\DoctrineORM\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
