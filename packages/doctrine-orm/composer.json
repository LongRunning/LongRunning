    "require": {
        "php": "^7.4 || ^8.0",
        "long-running/core": "^1.0",
        "psr/log": "^1.1.0",
        "doctrine/orm": "^2.7.3",
        "symfony/yaml": "^4.4 || ^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/framework-bundle": "^4.4.12 || ^5.3.7",
        "symfony/phpunit-bridge": "^4.4.30 || ^5.3.7",
        "doctrine/doctrine-bundle": "^2.3.1"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<2.3.1",
        "symfony/config": "<4.4.12 || >=5.3.0 <5.3.7",
        "symfony/dependency-injection": "<4.4.26 || >=5.3.0 <5.3.7",
        "symfony/http-foundation": "<4.4.30 || >=5.3.0 <5.3.7"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "When using the Symfony bundle"
    },
    "autoload": {
        "psr-4": {
            "LongRunning\\DoctrineORM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LongRunning\\DoctrineORM\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
