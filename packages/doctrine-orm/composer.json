{
    "name": "long-running/doctrine-orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "https://matthiasnoback.nl/"
        }
    ],
    "require": {
        "php": "^8.0",
        "long-running/core": "^1.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "doctrine/orm": "^2.7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "doctrine/doctrine-bundle": "^2.5"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<2.3.1"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "When using the Symfony bundle"
    },
    "autoload": {
        "psr-4": {
            "LongRunning\\DoctrineORM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LongRunning\\DoctrineORM\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "ocramius/package-versions": true,
            "composer/package-versions-deprecated": true
        }
    }
}
