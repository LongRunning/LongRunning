    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "https://matthiasnoback.nl/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "long-running/core": "^1.0",
        "psr/log": "^1.1.0",
        "sentry/sdk": "^3.1",
        "symfony/yaml": "^4.4 || ^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/framework-bundle": "^4.4.12 || ^5.3",
        "sentry/sentry-symfony": "^4.0"
    },
    "conflict": {
        "sentry/sentry-symfony": "<4.0"
    },
    "suggest": {
        "sentry/sentry-symfony": "When using the Symfony bundle"
    },
    "autoload": {
        "psr-4": {
            "LongRunning\\Sentry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LongRunning\\Sentry\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
