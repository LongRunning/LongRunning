    "require": {
        "php": "^8.0",
        "long-running/core": "^1.0",
        "psr/log": "^1.1.0",
        "sentry/sdk": "^3.1",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/framework-bundle": "^5.3.7",
        "symfony/phpunit-bridge": "^5.3.7",
        "sentry/sentry-symfony": "^4.0"
    },
    "conflict": {
        "sentry/sentry": "<3.1.3",
        "sentry/sentry-symfony": "<4.0",
        "symfony/config": "<5.3.7",
        "symfony/dependency-injection": "<5.3.7",
        "symfony/http-foundation": "<5.3.7",
        "symfony/options-resolver": "<5.3.7"
    },
    "suggest": {
        "sentry/sentry-symfony": "When using the Symfony bundle"
    },
    "autoload": {
        "psr-4": {
            "LongRunning\\Sentry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LongRunning\\Sentry\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
